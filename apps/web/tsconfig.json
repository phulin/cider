{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"types": ["vinxi/types/client"],
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src/**/*", "app.config.ts"]
}
